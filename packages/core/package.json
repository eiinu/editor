{
  "name": "@eiinu/editor-core",
  "version": "0.0.1",
  "description": "A web editor based on slatejs.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --emitDeclarationOnly && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "eiinu",
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "is-hotkey": "^0.2.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "slate": "^0.110.2",
    "slate-react": "^0.111.0",
    "slate-history": "^0.110.3"
  }
}
